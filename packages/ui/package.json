  "devDependencies": {
    "cross-env": "6.0.3",
    "cypress": "3.1.4",
    "cypress-image-snapshot": "3.0.0",
    "eslint-plugin-cypress": "2.2.0",
    "http-proxy-middleware": "0.19.1",
    "http-server": "0.11.1",
    "react-app-rewired": "2.1.3",
    "react-scripts": "3.2.0",
    "serve": "10.1.1",
    "start-server-and-test": "1.7.11"
  },
  "dependencies": {
    "@toiletmap/api-client": "3.0.0",
    "@toiletmap/leaflet-control-geocoder": "3.0.0",
    "@toiletmap/opening-hours": "3.0.0",
    "auth0-js": "9.10.0",
    "core-js": "2.6.3",
    "js-cookie": "2.2.1",
    "leaflet": "1.5.1",
    "leaflet-loading": "0.1.24",
    "leaflet.locatecontrol": "0.67.0",
    "leaflet.markercluster": "1.4.1",
    "lodash": "4.17.15",
    "normalize.css": "8.0.1",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-addons-update": "15.6.2",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.1",
    "react-leaflet": "2.4.0",
    "react-redux": "6.0.0",
    "react-responsive": "6.1.1",
    "react-router-dom": "4.3.1",
    "react-router-ga": "1.1.0",
    "react-router-hash-link": "1.2.2",
    "redux": "4.0.4",
    "redux-saga": "1.0.0"
  },
  "scripts": {
    "dev": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "start": "http-server ./build -p 5000 -a localhost --proxy https://gbptm-unity.herokuapp.com",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired build",
    "cy:open": "cypress open --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run --config baseUrl=http://localhost:5000,video=false",
    "test:integration": "start-server-and-test start http://localhost:5000 cy:run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
