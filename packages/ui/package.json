{
  "name": "@toiletmap/ui",
  "version": "3.3.0",
  "private": true,
  "main": "index.js",
  "devDependencies": {
    "@babel/node": "7.8.7",
    "bestzip": "2.1.5",
    "cordova": "9.0.0",
    "cpx": "1.5.0",
    "cypress": "4.3.0",
    "cypress-image-snapshot": "3.1.1",
    "env-cmd": "10.1.0",
    "eslint-plugin-cypress": "2.10.3",
    "http-proxy-middleware": "1.0.3",
    "http-server": "0.12.1",
    "react-app-rewired": "2.1.5",
    "react-scripts": "3.4.1",
    "serve": "11.3.0",
    "start-server-and-test": "1.10.11"
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.41",
    "@apollo/link-context": "2.0.0-beta.3",
    "@apollo/link-error": "2.0.0-beta.3",
    "@auth0/cordova": "0.4.3",
    "@toiletmap/opening-hours": "3.3.0",
    "auth0-js": "9.13.0",
    "concurrently": "5.1.0",
    "cordova-android": "8.1.0",
    "cordova-ios": "5.1.1",
    "cordova-plugin-customurlscheme": "5.0.1",
    "cordova-plugin-geolocation": "4.0.2",
    "cordova-plugin-safariviewcontroller": "1.6.0",
    "cordova-plugin-whitelist": "1.3.4",
    "core-js": "3.6.4",
    "graphql.macro": "1.4.2",
    "js-cookie": "2.2.1",
    "leaflet": "1.6.0",
    "leaflet-control-geocoder": "1.12.1",
    "leaflet-loading": "0.1.24",
    "leaflet.locatecontrol": "0.71.1",
    "leaflet.markercluster": "1.4.1",
    "lodash": "4.17.15",
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "query-string": "5.1.1",
    "react": "16.13.1",
    "react-addons-update": "15.6.2",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-leaflet": "2.6.3",
    "react-responsive": "8.0.3",
    "react-router-dom": "5.1.2",
    "react-router-ga": "1.2.3",
    "react-router-hash-link": "1.2.2",
    "resize-observer-polyfill": "1.5.1"
  },
  "scripts": {
    "dev": "react-app-rewired start",
    "start": "http-server ./build -p 5000 -a localhost --proxy https://gbptm-stage.herokuapp.com",
    "build": "REACT_APP_BAKED_BACKEND= react-app-rewired build",
    "cy:open": "cypress open --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run --config baseUrl=http://localhost:5000,video=false",
    "test:integration": "start-server-and-test start http://localhost:5000 cy:run",
    "nopostinstall": "yarn configure && cordova prepare",
    "configure": "env-cmd --silent babel-node --presets @babel/preset-react tasks/cordova-config.js",
    "prebuild-phonegap": "yarn prebuild-cordova",
    "build-phonegap": "yarn configure && bestzip phonegap.zip -r www/* package.json config.xml",
    "prebuild-cordova": "yarn build && cpx \"build/**/*\" www/",
    "build-cordova": "yarn configure && cordova requirements && cordova build",
    "predev-cordova": "yarn configure --dev && cordova requirements",
    "dev-cordova": "concurrently -n 'ReactDevServer,AndroidDevInstall' 'yarn dev' 'cordova run android'",
    "dev-cordova-ios": "concurrently -n 'ReactDevServer,AndroidDevInstall' 'yarn dev' 'cordova run ios'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-geolocation": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "toiletmap",
        "ANDROID_SCHEME": "toiletmap",
        "ANDROID_HOST": "gbptm.eu.auth0.com",
        "ANDROID_PATHPREFIX": "/cordova/toiletmap/callback"
      },
      "cordova-plugin-safariviewcontroller": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
